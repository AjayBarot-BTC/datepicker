<%= form_for(@user) do |f| %>
  <% if @user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% @user.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
<div class='well'>
    <input type="text" data-behaviour='datepicker' >
</div>
  <div class="field">
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </div>
  <div class="field">
  <label for="exampleYear_of_Birth1">Date_of_Birth:</label>
  <br/>
  <%= f.text_field :date, 'data-behaviour' => 'datepicker', class: "form-control", placeholder: "Enter Date_of_Birth", id: "exampleYear_of_Birth" %>
</div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
